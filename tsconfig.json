{
  "compilerOptions": {
    "noEmit": true,
    "target": "esnext",
    "module": "nodenext",
    "rewriteRelativeImportExtensions": true,
    "erasableSyntaxOnly": true,
    "verbatimModuleSyntax": true,
    "strict": true,
    "allowJs": true,
    "noImplicitAny": true,
    "allowImportingTsExtensions": true,
    "pretty": true,
  }
}